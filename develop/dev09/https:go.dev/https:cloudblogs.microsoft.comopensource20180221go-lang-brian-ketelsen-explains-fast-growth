					scroll: true,
					pageView: true,
					onLoad: true,
					onUnload: true,
					click: true,
					scroll: true,
					resize: true,
					jsError: true
				}
			}
		}; 
		// Initialize OneDS SDK
		analytics.initialize( config, [] );
