		// 1DS initialization
		const analytics = new oneDS.ApplicationInsights();
		var config = {
			instrumentationKey: "b341ec446e65436485df678003ce82f7-1d36a7a4-ef54-4cf0-93eb-ba18b3241d36-7404",
			propertyConfiguration: {
				callback: {
					userConsentDetails: siteConsent ? siteConsent.getConsent : null
				},
			},
			webAnalyticsConfiguration:{
				coreData: {"pageName":"Ready, Set, Go lang: Brian Ketelsen explains Go\u2019s fast growing popularity","pageType":"Post"},
				urlCollectQuery: true,
