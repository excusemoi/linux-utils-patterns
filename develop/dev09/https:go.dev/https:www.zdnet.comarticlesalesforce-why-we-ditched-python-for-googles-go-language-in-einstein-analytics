        <path d="M785.337002,284.988732 L835.257259,284.988732 L835.257259,283.052344 C835.257259,270.823579 833.960629,264.367952 830.088747,258.234555 C825.882704,251.804947 819.771734,248.567627 811.083512,248.567627 C794.671538,248.567627 785.337002,261.453863 785.337002,284.341268 L785.337002,284.988732 Z M861.328927,341.354132 L878.083069,367.121602 C859.078834,382.574678 839.110131,390 815.927866,390 C768.58186,390 738,356.500989 738,304.636815 C738,275.011581 744.102966,255.350488 758.579,239.234937 C772.138588,224.107094 788.550563,217 810.454207,217 C829.440432,217 847.482199,223.458629 858.097357,234.426491 C873.22971,249.878566 880,272.107499 880,306.588214 C880,310.108737 880,311.390656 880,316.54535 L784.668677,316.54535 L784.668677,317.848284 C784.668677,343.272507 797.237785,357.783909 819.47459,357.783909 C834.284786,357.783909 848.131515,352.309985 861.328927,341.354132 L861.328927,341.354132 Z" id="Fill-6" fill="#FFFFFF"></path>
        <path d="M993.027231,219.671343 L982.073315,248.341683 L954.334882,248.341683 L954.334882,332.552104 C954.334882,353.511022 958.201913,359.007014 973.039891,359.007014 C976.907923,359.007014 980.774954,358.009018 988.520036,355.765531 L994,381.556112 C981.434153,386.743487 970.138616,389 958.851093,389 C936.928233,389 919.175956,379.312625 914.354189,364.804609 C911.775501,357.698397 911.451913,354.461924 911.451913,339.314629 L911.451913,248.341683 L895,248.341683 L895,220.301603 L911.451913,220.301603 C911.451913,204.180361 911.451913,193.222445 913.074863,180.968938 L956.912568,170 C955.279599,185.146293 954.334882,202.896794 954.334882,219.671343 L993.027231,219.671343" id="Fill-7" fill="#FFFFFF"></path>
